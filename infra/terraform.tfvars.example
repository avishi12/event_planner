# Copy this file to terraform.tfvars and update with your values
# cp terraform.tfvars.example terraform.tfvars

# AWS region to deploy to
aws_region = "us-east-1"

# EC2 instance type (t2.micro is free tier eligible)
instance_type = "t2.micro"

# Name of your SSH key pair (must be created in AWS first)
key_name = "your-key-pair-name"

# DockerHub images (update with your DockerHub username)
dockerhub_backend_image  = "eg244991/event-planner-backend:latest"
dockerhub_frontend_image = "eg244991/event-planner-frontend:latest"
